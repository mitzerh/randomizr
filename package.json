{
  "name": "randomizr",
  "version": "0.0.1",
  "description": "Random string generator",
  "main": "index.js",
  "scripts": {
    "test": "node test.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-cond-assign": [
        "error",
        "always"
      ]
    },
    "env": {
      "es6": true,
      "node": true
    },
    "globals": {}
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitzerh/randomizr.git"
  },
  "keywords": [
    "randomizer",
    "string",
    "unique"
  ],
  "author": "Helcon Mabesa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mitzerh/randomizr/issues"
  },
  "homepage": "https://github.com/mitzerh/randomizr#readme"
}
